# Telegram Bot Token from BotFather
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN

# OpenRouter API Key
OPENROUTER_API_KEY=YOUR_OPENROUTER_API_KEY

# Comma-separated list of allowed Telegram Chat IDs (leave empty to allow all)
ALLOWED_CHAT_IDS=

# Telegram Chat ID for sending error notifications
DEVELOPER_CHAT_ID=

# Database URL (Example for PostgreSQL)
# DATABASE_URL=postgresql+psycopg2://user:password@host:port/database
DATABASE_URL=sqlite:///family_assistant.db # Example for SQLite

# Other secrets or configurations can go here
# e.g., CALENDAR_API_KEY=...
