{
  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone=Australia/Sydney"]
    },
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "brave": {
      "command": "deno",
      "args": [
        "run",
        "-A",
        "npm:@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "$BRAVE_API_KEY"
      }
    },
    "python": {
      "command": "deno",
      "args": [
        "run",
        "-N",
        "-R=node_modules",
        "-W=node_modules",
        "--node-modules-dir=auto",
        "jsr:@pydantic/mcp-run-python",
        "stdio"
      ]
    },
    "homeassistant": {
    	"command": "mcp-proxy",
    	"env": {
    		"SSE_URL": "https://homeassistant.alexsmith.dev/mcp_server/sse",
    		"API_ACCESS_TOKEN": "$HOMEASSISTANT_API_KEY"
    	}
    }
  }
}
